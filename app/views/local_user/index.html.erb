<h1>LocalUsers</h1>


<% if @local_users.nil? %>
    No Users
<% else %>
<table class="table table-striped table-hover table-condensed table-responsive"><br>
  <tr>
    <th>Id</th>
    <th>Username</th>

  </tr>

  <% @local_users.each do |t| %>
      <tr>
        <td><%=	t.id  %></td>
        <td><%= t.username %></td>
      </tr>
  <% end %>

      <% end %>
</table>
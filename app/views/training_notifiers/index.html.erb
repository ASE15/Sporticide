<% content_for :title do %>
    <h1>
      Notifications
      <small>Your notifications</small>
    </h1>
<% end %>

<div class="box box-primary">
  <div class="box-body">

<table class="table table-striped table-hover table-condensed table-responsive"><br>
  <tr>
    <th>Notification</th>
    <th>Created</th>
  </tr>

  <% @notifications.each do |t| %>
      <tr>
        <td><%= t.system_log.log %></td>
        <td><%= t.created_at %></td>

      </tr>
  <% end %>
</table>

  </div>
</div>